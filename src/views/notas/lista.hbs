<div class="container p-4">
    <div class="row">

        {{#each notas}}
            <div class="col-md-3">
                <div class="card text-center">
                    <div class="card-body">
                        <h3 class="card-title text-uppercase custom-title-color">
                            {{titulo}}
                        </h3>
                        <p class="m-2">{{nota}}</p>
                        <p>{{timeago creado_en}}</p>
                        <a href="" class="btn btn-danger">Borrar Nota</a>
                        <a href="" class="btn btn-secondary">Editar</a>
                    </div>
                </div>
            </div>
        {{/each}}

    </div>
</div>